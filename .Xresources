! Fonts:
! I would go with lemon if it werent so damn tiny.
URxvt*font: xft:uushi:pixelsize=10
!! Most bitmap fonts dont have bold, nor do I need it.
URxvt*boldFont: xft:uushi:pixelsize=10

! Since urxvt ignores fontconfig settings.
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! urxvt scrolling options and cursor style:
URxvt*saveLines: 12000
URxvt*scrollstyle: plain
URxvt*scrollBar: false
URxvt*cursorBlink: true
URxvt*cursorUnderline: true

! urxvt perl extensions:
URxvt.perl-ext-common: default,matcher

! No, dont go to ISO mode for S-C.
URxvt.iso14755: False

! Keybindings.
URxvt.keysym.C-u: command:\033]720;1\007
URxvt.keysym.C-d: command:\033]721;1\007
! Somehow map it to some obscure key, and then map that key via Vim.
!URxvt.keysym.S-space: command: \033[1~

!enabling clickable links:
URxvt.matcher.button: 1
URxvt.urlLauncher: /usr/bin/firefox

! Highlight to end of last character, not the full line (particularly
! more convenient for yanking).
! Not working.
URxvt.highlightSelection: true

! If only i3 supported (real) transparency!
! Reason: nice to have just a /little/ blurred transparency to see through where I am.
!urxvt*depth: 100
!urxvt*background: rgba:0000/0000/0200/c800
! vim: ft=xdefaults
