!
! urxvt and dzen2 dotfile
! ~/.Xresources
! Name: nil
!
Xcursor.theme:    Vanilla-DMZ-AA

!urxvt color scheme:

URxvt*background: #2B2B2B
URxvt*foreground: #DEDEDE

URxvt*colorUL: #86a2b0

! black
URxvt*color0  : #2E3436
URxvt*color8  : #555753
! red
URxvt*color1  : #CC0000
URxvt*color9  : #EF2929
! green
URxvt*color2  : #4E9A06
URxvt*color10 : #8AE234
! yellow
URxvt*color3  : #C4A000
URxvt*color11 : #FCE94F
! blue
URxvt*color4  : #3465A4
URxvt*color12 : #729FCF
! magenta
URxvt*color5  : #75507B
URxvt*color13 : #AD7FA8
! cyan
URxvt*color6  : #06989A
URxvt*color14 : #34E2E2
! white
URxvt*color7  : #D3D7CF
URxvt*color15 : #EEEEEC

! Colorful Man pages.
! Doesnt seem to work.
!URxvt.colorIT:      #87af5f
!URxvt.colorBD:      #d7d7d7
!URxvt.colorUL:      #87afd7

! Fonts:
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

URxvt*font: xft:Consolas:size=13
URxvt*boldFont: xft:Consolas:bold:size=13
URxvt.letterSpace: -1
!URxvt*font: xft:Monospace:pixelsize=12
!URxvt*boldFont: xft:Monospace:bold:pixelsize=12

!urxvt extensions:

! Custom Perl Script Library
!URxvt.perl-lib: /home/nil/.config/nil/urxvt-perls
!loaded clipboard into /lib/urxvt/perl to see if the library is the problem.
URxvt.perl-ext-common: default,matcher,clipboard

! Keybindings.
URxvt.keysym.C-v: perl:clipboard:paste
URxvt.keysym.C-u: command:\033]720;1\007
URxvt.keysym.C-d: command:\033]721;1\007

!enabling clickable links:

URxvt.urlLauncher:      /usr/bin/google-chrome
URxvt.matcher.button:   1

!urxvt scrolling options and cursor style:

URxvt*saveLines: 12000
URxvt*scrollstyle:plain
URxvt*scrollBar: false
URxvt*cursorBlink: true
URxvt*cursorUnderline: true

! urxvt tabs settings:

URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 2
URxvt.tabbed.tab-bg: 8
URxvt.tabbed.autohide:  yes
URxvt.tabbed.new-button:  no
URxvt.tabbed.title: no

! dzen2 {{{

dzen2.font:       -*-fixed-*-*-*-*-*-*-*-*-*-*-*-*
dzen2.foreground: #22EE11
dzen2.background: black

! }}}
